    "@mantine/notifications": "^7.11.2",
    "dayjs": "^1.11.12",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.25.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
